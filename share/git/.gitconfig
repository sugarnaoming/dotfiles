[user]
	email = sakigithub@gmail.com
	name = sugarnaoming
[core]
  editor = vim
[color]
  ui = true
[alias]
  c = clone
  s = status
  st = stash
  stl = stash list
  stp = stash pop
  p = push
  pf = push --force
  pu = pull
  pup = pull --prune
  co = checkout
  cob = checkout -b
  cp = cherry-pick
  cpc = cherry-pick --continue
  b = branch
  ba = branch -a
  bav = branch -a -vvv
  bd = branch -d
  bdf = branch -D
  bm = branch -m
  a = add
  ap = add -p
  m = merge
  ma = merge --abort
  mnf = merge --no-ff
  reb = rebase
  rebid = rebase --ignore-date
  rebi = rebase -i
  rebc = rebase --continue
  reba = rebase --abort
  rem = remote
  remv = remote -v
  remp = remote prune
  res = reset
  resh = reset --hard
  ress = reset --soft
  cm = commit
  cma = commit --amend
  cam = commit --allow-empty -m
  cmm = commit -m
  f = fetch
  fa = fetch --all
  fap = fetch --all --prune
  rev = revert
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
[url "git@github.com:"]
    insteadOf = https://github.com/
